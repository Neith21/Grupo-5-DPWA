@model CategoryModel
@{
    ViewData["Title"] = "Editar Categorias";
}

<h1>Editar Categoría</h1>

<div class="container">
    <div class="row">
        <div class="col-md-12">
            <form asp-action="Edit">
                @Html.AntiForgeryToken()
                <div class="mb-3">
                    <label asp-for="CategoryName" class="form-label">Nombre</label>
                    <input asp-for="CategoryName" type="text" class="form-control">
                </div>
                <button type="submit" class="btn btn-primary">Editar </button>
            </form>

        </div>
    </div>
</div>